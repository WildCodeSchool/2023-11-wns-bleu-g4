{
  "name": "next-frontend-template",
  "version": "1.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"**/*.{js,mjs,ts,tsx}\" --cache --cache-strategy=content --cache-location=.eslintjscache",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier \"**/*.{js,ts,tsx,json,yml,css}\" --check --cache --cache-strategy=content --cache-location=.prettiercache",
    "prettier:fix": "npm run prettier -- --write",
    "codegen": "graphql-codegen --config codegen.yml --watch"
  },
  "author": "Jeremy Dohin",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.9.11",
    "@chakra-ui/next-js": "^2.2.0",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@heroicons/react": "^2.1.3",
    "@types/node": "20.11.28",
    "autoprefixer": "^10.4.19",
    "framer-motion": "^11.1.7",
    "graphql": "^16.8.1",
    "next": "^14.2.3",
    "postcss": "^8.4.38",
    "react": "^18.2.0",
    "react-click-away-listener": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-responsive-carousel": "^3.2.23",
    "react-toastify": "^9.1.3",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.2.2",
    "@graphql-codegen/introspection": "3.0.1",
    "@graphql-codegen/typescript": "3.0.2",
    "@graphql-codegen/typescript-operations": "3.0.2",
    "@graphql-codegen/typescript-react-apollo": "3.3.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14"
  }
